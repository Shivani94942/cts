<?xml version="1.0"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
 <display-name>Archetype Created Web Application</display-name>
 <servlet>
  <servlet-name>CustomerView</servlet-name>
  <display-name>CustomerView</display-name>
  <servlet-class>customer1.view.CustomerView</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>CustomerController</servlet-name>
  <display-name>CustomerController</display-name>
  <servlet-class>customer1.controller.CustomerController</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>HomeController</servlet-name>
  <display-name>HomeController</display-name>
  <servlet-class>customer1.controller.HomeController</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>CustomerUpdateController</servlet-name>
  <display-name>CustomerUpdateController</display-name>
  <servlet-class>customer1.controller.CustomerUpdateController</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>UpdateController</servlet-name>
  <display-name>UpdateController</display-name>
  <servlet-class>customer1.controller.UpdateController</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>DeleteController</servlet-name>
  <display-name>DeleteController</display-name>
  <servlet-class>customer1.controller.DeleteController</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>CustomerView</servlet-name>
  <url-pattern>/success.view</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CustomerController</servlet-name>
  <url-pattern>/controller.do</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>HomeController</servlet-name>
  <url-pattern>/homecontroller.do</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CustomerUpdateController</servlet-name>
  <url-pattern>/customerUpdate.do</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>UpdateController</servlet-name>
  <url-pattern>/updateCustomer.do</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>DeleteController</servlet-name>
  <url-pattern>/deleteController.do</url-pattern>
 </servlet-mapping>
</web-app>
